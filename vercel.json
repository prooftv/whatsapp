{
  "functions": {
    "api/webhook.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/webhook",
      "destination": "https://arqeiadudzwbmzdhqkit.supabase.co/functions/v1/webhook"
    },
    {
      "source": "/admin/(.*)",
      "destination": "https://arqeiadudzwbmzdhqkit.supabase.co/functions/v1/admin-api/$1"
    },
    {
      "source": "/moments/(.*)",
      "destination": "/public/moments/$1"
    },
    {
      "source": "/moments",
      "destination": "/public/moments/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}